{
  "name": "@chroberino/node-win-verify-trust",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "version": "0.1.0",
  "description": "Check the signature of a file using the WinVerifyTrust API",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "binding.gyp"
  ],
  "engines": {
    "node": ">=20.12.2",
    "npm": "^10.5.0"
  },
  "gypfile": true,
  "scripts": {
    "install": "node-gyp rebuild",
    "prepublishOnly": "npm install --ignore-scripts && npm run build",
    "build": "tsc",
    "clean": "node-gyp clean && rimraf dist build",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "node --test",
    "declare": "tsc --emitDeclarationOnly --outDir \"./dist\""
  },
  "keywords": [
    "WinVerifyTrust",
    "signature",
    "sign",
    "windows",
    "trust"
  ],
  "author": {
    "name": "Anthony Beaumont",
    "email": "dev.xan105@outlook.be",
    "url": "https://xan105.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chroberino/node-win-verify-trust.git"
  },
  "bugs": {
    "url": "https://github.com/chroberino/node-win-verify-trust/issues"
  },
  "homepage": "https://github.com/chroberino/node-win-verify-trust#readme",
  "dependencies": {
    "node-addon-api": "^8.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "typescript": "^5.4.0",
    "eslint": "^9.5.0",
    "typescript-eslint": "^8.26.1",
    "globals": "^16.0.0",
    "rimraf": "^5.0.0"
  }
}
